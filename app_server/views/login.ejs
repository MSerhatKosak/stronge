<body id="login-page">

    <div class="form-box">
        <div class="form-logo">
            <p class="text-center"><img src="img/logo1.png" alt="logo"></p>
        </div>
        <form action="login" method="POST">
            <input type="text" name="ad" class="form-control" placeholder="Kullanıcı Ad">
            <input type="password" name="password" class="form-control" placeholder="Şifre">
            <input type="checkbox"> Kaydet
            <input type="submit" class="btn btn-social" value="Giriş">
        </form>
        <a href="#" class="login-action" title="Forgot password">
            <p>Forgot your password?</p>
        </a>
        <a href="signup" class="login-action" title="Signi in">
            <p>Üye degil misin ?</p>
        </a>
    </div>

    <footer class="login-footer text-center">
        <div class="col-xs-3">
            <a class="login-action" href="#" title="language">LANGUAGE</a>
        </div>
        <div class="col-xs-3">
            <a class="login-action" href="#" title="terms">TERMS</a>
        </div>
        <div class="col-xs-3">
            <a class="login-action" href="#" title="privacy">PRIVACY</a>
        </div>
        <div class="col-xs-3">
            <a class="login-action" href="#" title="careers">CAREERS</a>
        </div>
    </footer>

</body>


</html>
